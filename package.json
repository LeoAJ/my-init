{
  "name": "my-init",
  "version": "1.3.0",
  "description": "CLI to create a customize package.json file",
  "license": "MIT",
  "keywords": [
    "cli",
    "npm-init",
    "package.json"
  ],
  "scripts": {
    "dev": "babel -w mi.js -d lib",
    "build": "babel mi.js -d lib",
    "coverage": "jest --coverage",
    "test": "jest test/"
  },
  "bin": {
    "mi": "cli.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "jest": "^21.2.1"
  },
  "dependencies": {
    "fs-extra": "^4.0.2",
    "npmlog": "^4.1.2",
    "read-pkg": "^3.0.0",
    "write-json-file": "^2.3.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
